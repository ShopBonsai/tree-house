{
  "name": "@tree-house/errors",
  "version": "3.0.0",
  "description": "NodeJS default error definitions with an error parser utility function",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/ShopBonsai/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/ShopBonsai/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopBonsai/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "yarn build",
    "prepublish": "yarn build",
    "pretest": "yarn build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@tree-house/translations": "^2.3.0",
    "http-status": "~1.6.2",
    "i18n": "0.15.1",
    "safe-json-stringify": "~1.2.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/i18n": "~0.13.6",
    "@types/jest": "29.5.1",
    "@types/safe-json-stringify": "1.1.2",
    "@types/uuid": "9.0.1",
    "coveralls": "3.1.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "rimraf": "~5.0.0",
    "supertest": "~6.1.6",
    "ts-jest": "29.1.0",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "~5.11.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=16.x"
  }
}
