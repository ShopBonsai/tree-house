{
  "name": "@tree-house/errors",
  "version": "1.3.0",
  "description": "NodeJS default error definitions with an error parser utility function",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/knor-el-snor/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/knor-el-snor/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knor-el-snor/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@tree-house/translations": "^1.2.0",
    "http-status": "~1.5.0",
    "i18n": "0.13.3",
    "safe-json-stringify": "~1.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/http-status": "~1.1.2",
    "@types/i18n": "~0.13.0",
    "@types/jest": "26.0.23",
    "@types/safe-json-stringify": "1.1.2",
    "@types/uuid": "8.3.0",
    "coveralls": "3.1.0",
    "husky": "6.0.0",
    "jest": "27.0.1",
    "rimraf": "~3.0.2",
    "supertest": "~6.1.3",
    "ts-jest": "27.0.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "~5.11.2",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=12.x"
  }
}
