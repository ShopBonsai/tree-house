{
  "name": "@tree-house/core",
  "version": "5.0.0",
  "description": "NodeJS utilities and handy helpers extending ExpressJS functionalities",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/ShopBonsai/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/ShopBonsai/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopBonsai/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "yarn build",
    "start": "yarn build && node dist/index.js",
    "pretest": "yarn build",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn build"
    }
  },
  "dependencies": {
    "@godaddy/terminus": "4.12.0",
    "body-parser": "~1.20.2",
    "cors": "~2.8.5",
    "express": "~4.18.2",
    "express-rate-limit": "6.7.0",
    "helmet": "7.0.0",
    "js-yaml": "~4.1.0",
    "swagger-ui-express": "~4.6.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-serve-static-core": "4.17.34",
    "@types/jest": "29.5.1",
    "@types/js-yaml": "~4.0.5",
    "@types/supertest": "2.0.12",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "rimraf": "~5.0.0",
    "supertest": "^6.1.6",
    "ts-jest": "29.1.0",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18.x"
  }
}
