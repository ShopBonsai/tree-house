{
  "name": "@tree-house/core",
  "version": "1.0.0",
  "description": "NodeJS utilities and handy helpers extending ExpressJS functionalities",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "start": "npm run build && node build/index.js",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-rate-limit": "5.1.3",
    "helmet": "3.22.0",
    "https": "~1.0.0",
    "js-yaml": "~3.13.1",
    "rate-limit-redis": "1.7.0",
    "swagger-ui-express": "~4.0.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/express-rate-limit": "5.0.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.2.1",
    "@types/js-yaml": "~3.12.1",
    "@types/redis": "~2.8.13",
    "@types/supertest": "2.0.9",
    "coveralls": "3.1.0",
    "husky": "4.2.5",
    "jest": "25.5.3",
    "redis-mock": "0.49.0",
    "supertest": "^4.0.2",
    "ts-jest": "25.5.1",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.9.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "gitHead": "d12dfc7aa5b3dbba4693b391bf588202ba3f3a6d",
  "repository": {
    "type": "git",
    "url": "https://github.com/knor-el-snor/tree-house.git"
  },
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
