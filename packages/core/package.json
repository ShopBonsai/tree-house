{
  "name": "@tree-house/core",
  "version": "3.0.0",
  "description": "NodeJS utilities and handy helpers extending ExpressJS functionalities",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/ShopBonsai/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/ShopBonsai/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopBonsai/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist/*",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "start": "npm run build && node dist/index.js",
    "pretest": "npm run build",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@godaddy/terminus": "4.9.0",
    "body-parser": "~1.19.0",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-rate-limit": "5.4.0",
    "helmet": "4.6.0",
    "js-yaml": "~4.1.0",
    "rate-limit-redis": "2.1.0",
    "swagger-ui-express": "~4.1.6"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-rate-limit": "5.1.3",
    "@types/express-serve-static-core": "4.17.24",
    "@types/helmet": "4.0.0",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "~4.0.3",
    "@types/redis": "~2.8.32",
    "@types/supertest": "2.0.11",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "redis-mock": "0.56.3",
    "supertest": "^6.1.6",
    "ts-jest": "27.0.5",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=12.x"
  }
}
