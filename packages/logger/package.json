{
  "name": "@tree-house/logger",
  "version": "2.0.0",
  "description": "Simple and fast JSON logging library for node.js services.",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/knor-el-snor/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/knor-el-snor/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knor-el-snor/tree-house.git"
  },
  "license": "ISC",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./build",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "debug": "4.3.0",
    "json-stringify-safe": "5.0.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/faker": "5.1.2",
    "@types/jest": "26.0.7",
    "@types/json-stringify-safe": "5.0.0",
    "faker": "5.1.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.3",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "~5.11.2",
    "typescript": "3.9.7"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
