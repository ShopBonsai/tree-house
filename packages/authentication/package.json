{
  "name": "@tree-house/authentication",
  "version": "3.1.0",
  "description": "Tree House Authentication",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "API",
    "Secure",
    "Customisable"
  ],
  "homepage": "https://github.com/ShopBonsai/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/ShopBonsai/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopBonsai/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist/*",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "express-session": "~1.17.2",
    "jsonwebtoken": "~8.5.1",
    "jwks-rsa": "2.0.5",
    "openid-client": "3.15.9",
    "qrcode": "~1.4.4",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "~2.4.2",
    "@types/express-session": "~1.17.4",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "~8.5.5",
    "@types/qrcode": "~1.4.1",
    "@types/speakeasy": "~2.0.6",
    "coveralls": "3.1.1",
    "husky": "7.0.2",
    "jest": "27.2.4",
    "node-mocks-http": "~1.11.0",
    "supertest": "~6.1.6",
    "ts-jest": "27.0.5",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=12.x"
  }
}
