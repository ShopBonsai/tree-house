    "jest": "25.5.3",
    "rimraf": "~3.0.2",
    "ts-jest": "^25.1.0",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "~5.11.2",
    "typescript": "3.9.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
