{
  "name": "@tree-house/serializer",
  "version": "1.0.1",
  "description": "Json de/serialization made for consistency",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
