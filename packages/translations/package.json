{
  "name": "@tree-house/translations",
  "version": "2.0.2",
  "description": "Import and use translations through json files",
  "keywords": [
    "NodeJS",
    "Treehouse",
    "Json",
    "Translations"
  ],
  "homepage": "https://github.com/ShopBonsai/tree-house/tree/master/packages/core#readme",
  "bugs": {
    "url": "https://github.com/ShopBonsai/tree-house/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShopBonsai/tree-house.git"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist/*",
    "lint": "tslint 'src/**/*.ts'",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts",
    "test:watch": "jest --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "lodash": "~4.17.21"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/lodash": "~4.14.175",
    "jest": "27.2.4",
    "nock": "~13.1.3",
    "rimraf": "~3.0.2",
    "ts-jest": "27.0.5",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "~5.11.2",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=12.x"
  }
}
